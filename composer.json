{
    "name": "cultuurnet/demo-silex-oauth",
    "description": "Demo app to illustrate functionality of the cultuurnet/symfony-security-oauth lib.",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
      {
        "name": "Nicolas Leroy",
        "email": "nicolas.leroy@gmail.com"
      },
      {
        "name": "Jonas Verhaeghe",
        "email": "Jonas@cultuurnet.be"
      },
      {
        "name": "Kristof Coomans",
        "email": "kristof@2dotstwice.be"
      }
    ],
    "require": {
      "cultuurnet/symfony-security-oauth": "~0.1",
      "cultuurnet/symfony-security-oauth-redis": "~0.1",
      "silex/silex": "~1.3",
      "deralex/yaml-config-service-provider": "~1.0.1",
      "cultuurnet/symfony-security-oauth-uitid": "~0.1",
      "cultuurnet/silex-service-provider-oauth": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\DemoSilexOAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\DemoSilexOAuth\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
